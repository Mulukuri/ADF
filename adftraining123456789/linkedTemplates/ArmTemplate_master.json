{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adftraining123456789"
		},
		"AzureDataLakeSQL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeSQL'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricks2_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks2'"
		},
		"AzureSqlDatabase11_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase11'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"linked_service for the blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_service for the blob'"
		},
		"linked_service_adl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_service_adl'"
		},
		"linked_service_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_service_sql'"
		},
		"AzureDataLakeSQL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeadftraining.dfs.core.windows.net/"
		},
		"linked_service_adl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeadftraining.dfs.core.windows.net/"
		},
		"trigger1_properties_pipeline8_parameters_temp_arry": {
			"type": "array",
			"defaultValue": [
				1,
				2,
				3,
				4,
				5,
				6,
				7,
				8,
				9
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adftraining123456789_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeSQL_accountKey": {
						"value": "[parameters('AzureDataLakeSQL_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricks2_accessToken": {
						"value": "[parameters('AzureDatabricks2_accessToken')]"
					},
					"AzureSqlDatabase11_connectionString": {
						"value": "[parameters('AzureSqlDatabase11_connectionString')]"
					},
					"linked_service for the blob_connectionString": {
						"value": "[parameters('linked_service for the blob_connectionString')]"
					},
					"linked_service_adl_accountKey": {
						"value": "[parameters('linked_service_adl_accountKey')]"
					},
					"linked_service_sql_connectionString": {
						"value": "[parameters('linked_service_sql_connectionString')]"
					},
					"AzureDataLakeSQL_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeSQL_properties_typeProperties_url')]"
					},
					"linked_service_adl_properties_typeProperties_url": {
						"value": "[parameters('linked_service_adl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adftraining123456789_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"trigger1_properties_pipeline8_parameters_temp_arry": {
						"value": "[parameters('trigger1_properties_pipeline8_parameters_temp_arry')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adftraining123456789_ArmTemplate_0')]"
			]
		},
		{
			"name": "adftraining123456789_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adftraining123456789_ArmTemplate_1')]"
			]
		}
	]
}